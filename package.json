{
  "name": "@newrelic/mysql",
  "version": "4.0.0",
  "description": "MySQL instrumentation for the New Relic Node agent.",
  "main": "index.js",
  "scripts": {
    "test": "npm run versioned",
    "versioned": "npm run versioned:npm6",
    "versioned:npm6": "versioned-tests --minor -i 2 'tests/versioned/**/*.tap.js'",
    "versioned:npm7": "versioned-tests --minor --all -i 2 'tests/versioned/**/*.tap.js'",
    "lint": "eslint *.js lib tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newrelic/node-newrelic-mysql.git"
  },
  "keywords": [
    "newrelic",
    "instrumentation",
    "mysql",
    "apm"
  ],
  "author": "New Relic Node.js agent team <nodejs@newrelic.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/newrelic/node-newrelic-mysql/issues"
  },
  "homepage": "https://github.com/newrelic/node-newrelic-mysql#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@newrelic/test-utilities": "^5.1.0",
    "async": "^3.2.0",
    "eslint": "^5.7.0",
    "generic-pool": "^2.5.4",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "newrelic": "^7.5.2",
    "tap": "^15.0.9"
  },
  "peerDependencies": {
    "newrelic": ">=6.11.0"
  },
  "files": [
    "index.js",
    "nr-hooks.js",
    "lib/*",
    "*.md"
  ]
